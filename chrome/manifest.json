{
  "name":
  "SLVSH PowerUp",

  "description":
  "Adds sugar functionality to SLVSH's website, slvsh.com",

  "version":
  "0.0.3",

  "manifest_version":
  2,

  "icons": {

  },

  "content_scripts": [
    {

      "matches":
      [
      "*://*.slvsh.com/*"],

      "exclude_matches":
      [],

      "css":
      ["styles.css", "loader.css"],

      "js":
      ["jquery.js", "content_clean.js"],

      "run_at":
      "document_end"
    }
  ],

  "permissions": [
    "storage",
    "*://*.slvsh.com/*"
  ]
}
